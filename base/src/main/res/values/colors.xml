<?xml version="1.0" encoding="utf-8"?>
<resources>
  <!-- Theme & Accent -->
  <color name="theme">@color/red</color>
  <color name="dark_theme">@color/dark_red</color>
  <color name="accent">@color/black</color>
  <!-- Toolbar-->
  <color name="toolbar_background">@color/theme</color>
  <color name="toolbar_text">@color/white</color>
  <color name="toolbar_sub_text">@color/white</color>
  <!-- Navigation -->
  <color name="nav_background">@color/white</color>
  <color name="nav_active">@color/theme</color>
  <color name="nav_inactive">@color/grey400</color>
  <!-- Progressbar -->
  <integer-array name="progressBarColors">
    <item>@color/theme</item>
    <item>@color/accent</item>
  </integer-array>
  <!-- Auth -->
  <color name="errorColor">@color/grey500</color>
  <!-- Empty view -->
  <color name="empty_view_text">@color/black_alpha_9</color>
</resources>